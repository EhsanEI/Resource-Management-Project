<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="businesslogic.support.SystemConfiguration" table="SystemConfiguration" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="backupFormat" column="BackupFormatID" class="businesslogic.support.BackupFormat" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="date" column="DateID" class="businesslogic.utility.Date" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		
		<property name="backupPreiodDays" column="BackupPreiodDays" type="integer" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
