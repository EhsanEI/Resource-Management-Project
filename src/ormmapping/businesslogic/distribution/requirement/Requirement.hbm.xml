<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="businesslogic.distribution.requirement.Requirement" table="Requirement" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		
		<property name="resourceName" column="ResourceName" type="string" length="255" not-null="false" lazy="false"/>
		<property name="resourceType" column="ResourceType" type="string" length="255" not-null="false" lazy="false"/>
		<property name="quantity" column="Quantity" type="integer" not-null="true" lazy="false"/>
		<set name="ORM_Date" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="RequirementID" not-null="true"/>
			<one-to-many class="businesslogic.utility.Date"/>
		</set>
	</class>
</hibernate-mapping>
