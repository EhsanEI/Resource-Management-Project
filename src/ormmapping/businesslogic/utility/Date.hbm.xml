<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="businesslogic.utility.Date" table="`Date`" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		
		<property name="second" column="Second" type="integer" not-null="true" lazy="false"/>
		<property name="minute" column="Minute" type="integer" not-null="true" lazy="false"/>
		<property name="hour" column="Hour" type="integer" not-null="true" lazy="false"/>
		<property name="day" column="Day" type="integer" not-null="true" lazy="false"/>
		<property name="month" column="Month" type="integer" not-null="true" lazy="false"/>
		<property name="year" column="Year" type="integer" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
